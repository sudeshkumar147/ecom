{
    "name": "eriser/ecomerce",
    "description": "this is the solution for ecomerce",
	"keywords": ["eriser-ecom", "eriser","ecomerce"],
    "support": {
        "issues": "https://github.com/laravel/sudeshkumar147/eriser-ecom/issues",
        "source": "https://github.com/laravel/sudeshkumar147/eriser-ecom"
    },
    "type": "package",
	"license": "MIT",
    "authors": [
        {
            "name": "sudeshkumar147",
            "email": "sudeshkumar147@gmail.com"
        }
    ],
    "require": {
		"php": ">=5.6.0"
	},
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4" : {
            "Eriser\\Ecomerce\\" : "src/"
        }
    },
    "autoload-dev": {
        
        "psr-4": {
            "Eriser\\Tests\\": "tests/"
        }
    },

    "config": {
        "sort-packages": true
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
